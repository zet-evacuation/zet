<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">

    <activeProfiles>
        <activeProfile>github</activeProfile>
    </activeProfiles>

    <profiles>
        <profile>
            <id>github</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <url>https://repo1.maven.org/maven2</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>github-zetool</id>
                    <name>GitHub zetool Apache Maven Packages</name>
                    <url>https://maven.pkg.github.com/zetool/common</url>
                </repository>
                <repository>
                    <id>github-zet</id>
                    <name>GitHub zet evacuation tool Apache Maven Packages</name>
                    <url>https://maven.pkg.github.com/zet-evacuation/zet</url>
                </repository>
            </repositories>
        </profile>
    </profiles>

    <servers>
        <server>
            <id>github-zetool</id>
            <username>zet-bot</username>
            <password>${env.GH_PACKAGE_ACCESS}</password>
        </server>
        <server>
            <id>github-zet</id>
            <username>zet-bot</username>
            <password>${env.GH_PACKAGE_ACCESS}</password>
        </server>
    </servers>
</settings>
